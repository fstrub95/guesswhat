{
  "name" : "Guesser (no cell) : object (proj:0), FiLM (head:256 [3*3]), Pooling (conv:128), fusion (proj:0), score (sum), cell(none)",
  "model": {

    "type" : "guesser2",

    "object":
    {
      "no_categories": 90,
      "cat_emb_dim": 64,

      "spat_dim": 8,
      "spat_emb_dim": 64,

      "obj_emb_dim": 0
    },

    "dialogue":
    {
      "word_embedding_dim" : 300,
      "glove" : false,
      "rnn_state_size": 800,
      "use_lstm": false,
      "bidirectional" : true,
      "max_pool" : false,
      "layer_norm" : true
    },

    "reading_unit": {
        "reading_unit_type" : "only_question"
    },


    "image": {
      "image_input": "conv",
      "dim": [14,14,1024],
      "normalize": false,

      "attention": {
        "mode": "none"
      },

      "film_block": {
        "stem": {
          "spatial_location": true,
          "conv_out": 256,
          "conv_kernel": [3,3]
        },

        "resblock": {
          "feature_size": [128,128,128,128],
          "spatial_location": true,
          "kernel1": [1,1],
          "kernel2": [3,3]
        },

        "head": {
          "spatial_location": true,
          "conv_out": 256,
          "conv_kernel": [3,3],

          "attention": {
            "mode": "conv_pooling",
            "no_attention_mlp": 128
          }
        }
      }
    },

    "fusion": {
        "mode" : "none",
        "visual_dialogue_projection" : 0
    },

    "regularizer":
    {
      "dropout_keep_prob": 0.5,
      "dropout_list" : ["word_embedding_reg", "dialogue_reg", "object_embedding_reg"],
      "batchnorm_list" : []
    },

        "scoring_object" :
    {
      "use_scoring_mlp": false
    }

  },

  "optimizer": {
    "no_epoch": 20,
    "learning_rate": 1e-4,
    "batch_size": 64,
    "clip_val": 10,
    "weight_decay": 5e-6,
    "weight_decay_add": ["film_stack"],
    "weight_decay_remove": ["feedback_loop", "FiLM_layer", "pooling"]
  },

  "seed": -1

}