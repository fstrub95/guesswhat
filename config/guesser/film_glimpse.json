{
  "name" : "Film glimpse 2",
  "model": {
    "word_emb_dim": 300,
    "cat_emb_dim": 256,
    "obj_emb_hidden": 512,
    "dialog_emb_dim": 0,
    "spat_dim": 8,
    "no_categories": 90,

    "rnn_config":{
      "num_rnn_units": 1024,
      "use_lstm" : false,
      "bidirectional" : true,
      "max_pool" : false
    },

    "image": {
      "image_input": "conv",
      "dim": [
        14,
        14,
        1024
      ],
      "normalize": false,
      "attention": {
        "mode": "none"
      },
      "film_input": {
        "question": true
      },
      "film_block": {
        "stem": {
          "spatial_location": true,
          "conv_out": 256,
          "conv_kernel": [
            3,
            3
          ]
        },
        "resblock": {
          "feature_size": [
            128,
            128,
            128,
            128
          ],
          "spatial_location": true,
          "kernel1": [
            1,
            1
          ],
          "kernel2": [
            3,
            3
          ]
        },
        "head": {
          "spatial_location": true,
          "conv_out": 1024,
          "conv_kernel": [
            1,
            1
          ],
          "no_mlp_units": 1024,
          "attention": {
            "mode": "mean",
            "no_attention_mlp": 256,
            "no_glimpses": 2
          }
        }
      }
    },
    "dropout_keep_prob": 0.5
  },

  "optimizer": {
    "no_epoch": 20,
    "learning_rate": 1e-4,
    "batch_size": 64,
    "clip_val": 5
  },

  "seed": -1

}