{
  "model": {
    "name" : "Baseline with image, dialogue used for attention, not reinjected after",
    "word_emb_dim": 300,
    "cat_emb_dim": 256,
    "obj_emb_hidden": 512,
    "dialog_emb_dim": 0,

    "spat_dim": 8,
    "no_categories": 90,

    "rnn_config":{
      "num_rnn_units": 1024,
      "use_lstm" : true,
      "bidirectional" : true,
      "max_pool" : false
    },

    "image":
    {
      "image_input": "conv",
      "dim": [14, 14, 1024],
      "normalize": false,

      "attention" : {
        "mode": "mean",
        "no_attention_mlp" : 256,
        "fuse_mode" : "concat",
        "reinject_dial":false
      }

    },
    "dropout_keep_prob": 0.5
    },

  "optimizer": {
    "no_epoch": 20,
    "learning_rate": 1e-4,
    "batch_size": 64,
    "clip_val": 5
  },

  "seed": -1

}
