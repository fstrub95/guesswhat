{
  "model": {

    "name" : "CLEVR with FiLM",
    "type" : "film",
    "split_question" : false,

    "inputs" :
    {
      "image" : true,
      "crop" : false
    },

    "film_input":
    {
      "question": true,
      "category": true,
      "spatial": false,
      "mask": false
    },

    "attention_input":
    {
      "question": true,
      "category": false,
      "spatial": false,
      "mask": true
    },

    "mlp_input":
    {
      "question": false,
      "category": true,
      "spatial": true,
      "mask": false
    },

    "image":
    {
      "image_input": "conv",
      "dim": [14, 14, 2048],
      "normalize": false,

      "attention" : {
        "mode": "none"
      }

    },

    "question": {
      "word_embedding_dim": 200,
      "rnn_state_size": 2048,
      "glove" : true,
      "bidirectional" : true,
      "max_pool" : false
    },

    "category": {
      "n_categories": 90,
      "embedding_dim": 200
    },

    "spatial": {
      "no_mlp_units": 30
    },

    "stem" : {
      "spatial_location" : true,
      "mask" : true,
      "conv_out": 256,
      "conv_kernel": [3,3]
    },

    "resblock" : {
      "no_resblock" : 4,
      "spatial_location" : true,
      "mask" : true,
      "kernel1" : [1,1],
      "kernel2" : [3,3]
    },

    "classifier" : {
      "spatial_location" : true,
      "mask" : true,
      "merge" : false,
      "conv_out": 1024,
      "conv_kernel": [1,1],
      "no_mlp_units": 1024,

      "attention" : {
        "mode": "glimpse",
        "no_attention_mlp" : 256,
        "no_glimpses" : 1
      }
    }

  },

  "optimizer": {
    "no_epoch": 12,
    "learning_rate": 3e-4,
    "batch_size": 64,
    "clip_val": 10.0,
    "weight_decay": 1e-05
  },

  "dropout_keep_prob" : 0.5,
  "status": ["success"],

  "seed": -1
}